{
  "id": null,
  "title": "RabbitMQ Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "timeseries",
      "title": "Consumers per Queue",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rabbitmq_queue_consumers",
          "legendFormat": "{{queue}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
