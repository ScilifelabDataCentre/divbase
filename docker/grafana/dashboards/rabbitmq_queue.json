    {
      "type": "timeseries",
      "title": "Current Queue Depth",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rabbitmq_queue_messages",
          "legendFormat": "{{queue}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 16 }
