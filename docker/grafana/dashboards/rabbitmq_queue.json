    {
      "type": "timeseries",
      "title": "Messages Published Rate (per second)",
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "rate(rabbitmq_queue_messages_published_total[1m])",
          "legendFormat": "{{queue}}",
          "interval": ""
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      },
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
